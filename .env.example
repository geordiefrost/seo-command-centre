# Application
VITE_APP_NAME=Bang SEO Command Centre
VITE_APP_VERSION=1.0.0
VITE_MOCK_MODE=false

# Database & Authentication (Supabase)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
SUPABASE_JWT_SECRET=your_supabase_jwt_secret

# PostgreSQL Database
POSTGRES_URL=postgres://postgres.your-project:password@aws-0-us-east-1.pooler.supabase.com:6543/postgres?sslmode=require&supa=base-pooler.x
POSTGRES_PRISMA_URL=postgres://postgres.your-project:password@aws-0-us-east-1.pooler.supabase.com:6543/postgres?sslmode=require&pgbouncer=true
POSTGRES_URL_NON_POOLING=postgres://postgres.your-project:password@aws-0-us-east-1.pooler.supabase.com:5432/postgres?sslmode=require
POSTGRES_HOST=db.your-project.supabase.co
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_postgres_password
POSTGRES_DATABASE=postgres

# API Endpoints
VITE_API_URL=https://api.seocommand.com
VITE_BIGQUERY_PROJECT_ID=bang-digital-seo

# SEO Data Services
VITE_DATAFORSEO_LOGIN=your_dataforseo_login
VITE_DATAFORSEO_PASSWORD=your_dataforseo_password
VITE_DATAFORSEO_API_URL=https://api.dataforseo.com/v3

# AI Services
VITE_OPENAI_API_KEY=your_openai_key
VITE_ANTHROPIC_API_KEY=your_anthropic_key
VITE_PERPLEXITY_API_KEY=your_perplexity_key

# Automation Services
VITE_APIFY_API_KEY=your_apify_key
VITE_FIRECRAWL_API_KEY=your_firecrawl_key

# Google Services
VITE_GOOGLE_CLIENT_ID=your_google_client_id
VITE_GOOGLE_CLIENT_SECRET=your_google_client_secret
VITE_GOOGLE_REDIRECT_URI=https://your-domain.com/auth/google/callback
VITE_GOOGLE_API_KEY=your_google_api_key
VITE_GOOGLE_WORKSPACE_DOMAIN=bangdigital.com.au

# External Integrations
VITE_AGENCY_ANALYTICS_API_KEY=your_aa_key
VITE_SLACK_WEBHOOK_URL=your_slack_webhook

# Feature Flags
VITE_ENABLE_AI_CONTENT=true
VITE_ENABLE_MIGRATIONS=true
VITE_ENABLE_COMPETITIVE=true